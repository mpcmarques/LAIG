<SCENE>

    <!-- Initial values -->
    <INITIALS>
        <frustum near="0.1" far="500"/>
        <translation x="-1" y="1" z="0"/>
        <rotation axis="x" angle="0"/>
        <rotation axis="y" angle="0"/>
        <rotation axis="z" angle="0"/>
        <scale sx="0.25" sy="0.25" sz="0.25"/>
        <reference length="5"/>
    </INITIALS>


    <!-- Illumination and clear colour -->
    <ILLUMINATION>
        <ambient r="0.2" g="0.2" b="0.2" a="1"/>
        <background r="0" g="0" b="0" a="1"/>
    </ILLUMINATION>

    <!-- Lights -->
    <LIGHTS>

        <LIGHT id="sun">
            <enable value="1"/>
            <position x="15" y="10" z="-45" w="1"/>
            <ambient r="0" g="0" b="0" a="1.0"/>
            <diffuse r="1" g="1" b="1" a="1.0"/>
            <specular r="1" g="1" b="1" a="1.0"/>
        </LIGHT>


        <LIGHT id="fire">
            <enable value="1"/>
            <position x="-6" y="4" z="0" w="1"/>
            <ambient r="0" g="0" b="0" a="1.0"/>
            <diffuse r="1" g="0.5764" b="0.1607" a="1.0"/>
            <specular r="1" g="0.5764" b="0.1607" a="1.0"/>
        </LIGHT>

    </LIGHTS>

    <!-- Textures -->
    <TEXTURES>

        <!-- Brick wall -->
        <TEXTURE id="brick_wall">
            <file path="images/brick_wall.jpg"/>
            <amplif_factor s="0.5" t="1"/>
        </TEXTURE>

        <!-- Brick wall full -->
        <TEXTURE id="brick_wall_full">
            <file path="images/brick_wall.jpg"/>
            <amplif_factor s="30" t="30"/>
        </TEXTURE>

        <!-- Concrete floor -->
        <TEXTURE id="concrete">
            <file path="images/concrete.jpg"/>
            <amplif_factor s="0.4" t="0.4"/>
        </TEXTURE>

        <!-- Cloth Texture-->
        <TEXTURE id="cloth">
            <file path="images/cloth.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Metal -->
        <TEXTURE id="metal">
            <file path="images/metal.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Galvanized Metal -->
        <TEXTURE id="galvanized_metal">
            <file path="images/galvanized_metal.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Blue concrete -->
        <TEXTURE id="blue_concrete">
            <file path="images/blue_concrete.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Mountains landscape -->
        <TEXTURE id="mountains">
            <file path="images/mountains.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Aluminium -->
        <TEXTURE id="aluminium">
            <file path="images/aluminium.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Concrete 2 -->
        <TEXTURE id="concrete2">
            <file path="images/concrete2.jpg"/>
            <amplif_factor s="0.5" t="0.5"/>
        </TEXTURE>

        <!-- Candle -->
        <TEXTURE id="candle">
            <file path="images/candle.jpg"/>
            <amplif_factor s="0.5" t="0.5"/>
        </TEXTURE>

        <!-- Fire -->
        <TEXTURE id="fire">
            <file path="images/fire.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

    </TEXTURES>


    <MATERIALS>
        <!-- floor -->
        <MATERIAL id="concrete">
            <shininess value="32"/>
            <specular r="0.7" g="0.7" b="0.7" a="1"/>
            <diffuse r="0.55" g="0.55" b="0.55" a="1"/>
            <ambient r="0.1" g="0.1" b="0.1" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- Metal material -->
        <MATERIAL id="metal">
            <shininess value="76"/>
            <specular r="0.77" g="0.77" b="0.77" a="1"/>
            <diffuse r="0.5" g="0.5" b="0.5" a="1"/>
            <ambient r="0.05" g="0.05" b="0.05" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- Black metal -->
        <MATERIAL id="blackMetal">
            <shininess value="38.4"/>
            <specular r="0.332741" g="0.328634" b="0.346435" a="1"/>
            <diffuse r="0.18275" g="0.17" b="0.22525" a="1"/>
            <ambient r="0.05375" g="0.05" b="0.06625" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- rubber -->
        <MATERIAL id="rubber">
            <shininess value="10"/>
            <specular r="0.7" g="0.7" b="0.7" a="1"/>
            <diffuse r="0.5" g="0.5" b="0.5" a="1"/>
            <ambient r="0.05" g="0.05" b="0.05" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- fire -->
        <MATERIAL id="fire">
            <shininess value="10"/>
            <specular r="1" g="1" b="0" a="1"/>
            <diffuse r="1" g="1" b="0" a="1"/>
            <ambient r="1" g="1" b="0" a="1"/>
            <emission r="1" g="1" b="0.0" a="1"/>
        </MATERIAL>

        <!-- Landscape -->
        <MATERIAL id="landscape">
            <shininess value="0.5"/>
            <specular r="1" g="1" b="1" a="1"/>
            <diffuse r="1" g="1" b="1" a="1"/>
            <ambient r="1" g="1" b="1" a="1"/>
            <emission r="1" g="1" b="1" a="1"/>
        </MATERIAL>

        <!-- A default material -->
        <MATERIAL id="defaultMaterial">
            <shininess value="1"/>
            <specular r="0.0" g="0.0" b="0.0" a="1"/>
            <diffuse r="0.4" g="0.4" b="0.4" a="1"/>
            <ambient r="0.4" g="0.4" b="0.4" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>
    </MATERIALS>

    <ANIMATIONS/>



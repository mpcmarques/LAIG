<SCENE>

    <!-- Initial values -->
    <INITIALS>
        <frustum near="0.1" far="500"/>
        <translation x="-1" y="1" z="0"/>
        <rotation axis="x" angle="0"/>
        <rotation axis="y" angle="0"/>
        <rotation axis="z" angle="0"/>
        <scale sx="0.25" sy="0.25" sz="0.25"/>
        <reference length="5"/>
    </INITIALS>


    <!-- Illumination and clear colour -->
    <ILLUMINATION>
        <ambient r="0.2" g="0.2" b="0.2" a="1"/>
        <background r="0" g="0" b="0" a="1" />
    </ILLUMINATION>

    <!-- Lights -->
    <LIGHTS>
        <LIGHT id="light0">
            <enable value="1"/>
            <position x="0" y="10" z="0" w="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
            <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
        </LIGHT>

        <LIGHT id="light1">
            <enable value="1"/>
            <position x="10" y="13" z="10" w="1"/>
            <ambient r="1" g="1" b="1" a="1.0"/>
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0"/>
            <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
        </LIGHT>

        <LIGHT id="light2">
            <enable value="1"/>
            <position  x="10" y="6" z="6.9" w="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1.0"/>
            <diffuse r="0.2" g="0.2" b="0.2" a="1.0"/>
            <specular r="1.0" g="1.0" b="1.0" a="1.0"/>
        </LIGHT>
    </LIGHTS>

    <!-- Textures -->
    <TEXTURES>

        <!-- Screen -->
        <TEXTURE id="pc-screen">
            <file path="images/pc-screen.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Keyboard -->
        <TEXTURE id="keyboard">
            <file path="images/keyboard.png"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

        <!-- Floor -->
        <TEXTURE id="floor">
            <file path="images/moon.jpg"/>
            <amplif_factor s="0.1" t="0.2"/>
        </TEXTURE>

        <!--Flag Texture-->
        <TEXTURE id="flag">
            <file path="images/flag.png"/>
            <amplif_factor s="1" t="1"/>
         </TEXTURE>

        <!-- Metal -->
        <TEXTURE id="metal">
            <file path="images/metal.jpg"/>
            <amplif_factor s="1" t="1"/>
        </TEXTURE>

         <!-- Haste Texture-->
        <TEXTURE id="haste">
             <file path="images/haste.jpg"/>
             <amplif_factor s="1" t="1"/>
        </TEXTURE>

         <TEXTURE id="space">
              <file path="images/space.jpg"/>
              <amplif_factor s="1" t="1"/>
         </TEXTURE>

         <!-- golden tin foil -->
         <TEXTURE id="tin_foil">
            <file path="images/tin_foil.jpg"/>
            <amplif_factor s="0.5" t="0.5"/>
         </TEXTURE>

         <!-- aluminium -->
         <TEXTURE id="aluminium">
            <file path="images/aluminium.jpg"/>
            <amplif_factor s="1" t="1"/>
         </TEXTURE>

    </TEXTURES>


    <MATERIALS>
        <!-- floor -->
        <MATERIAL id="floorMaterial">
            <shininess value="1"/>
            <specular r="0" g="0" b="0" a="1"/>
            <diffuse r="1.0" g="0.68" b="0.4" a="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- PC Screen Material -->
        <MATERIAL id="pcScreenMaterial">
            <shininess value="100"/>
            <specular r="0.2" g="0.2" b="0.2" a="1"/>
            <diffuse r="0.0" g="0.207" b="0.0" a="1"/>
            <ambient r="0.1" g="0.1" b="0.1" a="1"/>
            <emission r="1" g="1" b="1" a="1"/>
        </MATERIAL>

        <!-- Metal material -->
        <MATERIAL id="metal">
            <shininess value="0.4"/>
            <specular r="0.508273" g="0.508273" b="0.508273" a="1"/>
            <diffuse r="0.50754" g="0.50754" b="0.50754" a="1"/>
            <ambient r="0.19225" g="0.19225" b="0.19225" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- Gold material -->
        <MATERIAL id="gold">
            <shininess value="0.4"/>
            <specular r="0.75" g="0.555802" b="0.366065" a="1"/>
            <diffuse r="0.85" g="0.60648" b="0.22648" a="1"/>
            <ambient r="0.3" g="0.1995" b="0.0745" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- Black plastic -->
        <MATERIAL id="blackPlasticMaterial">
            <shininess value="32"/>
            <specular r="0.5" g="0.5" b="0.5" a="1"/>
            <diffuse r="0.01" g="0.01" b="0.01" a="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- rubber -->
        <MATERIAL id="rubber">
            <shininess value="10"/>
            <specular r="0.7" g="0.7" b="0.7" a="1"/>
            <diffuse r="0.4" g="0.4" b="0.4" a="1"/>
            <ambient r="0.2" g="0.2" b="0.2" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

        <!-- A default material -->
        <MATERIAL id="defaultMaterial">
            <shininess value="1"/>
            <specular r="0.0" g="0.0" b="0.0" a="1"/>
            <diffuse r="0.4" g="0.4" b="0.4" a="1"/>
            <ambient r="0.4" g="0.4" b="0.4" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>


        <!-- Ligh bulb Material -->
        <MATERIAL id="lightMaterial">
            <shininess value="0.6"/>
            <specular r="0.774597" g="0.774597" b="0.774597" a="1"/>
            <diffuse r="0.4" g="0.4" b="0.4" a="1"/>
            <ambient r="0.05" g="0.05" b="0.05" a="1"/>
            <emission r="0.0" g="0.0" b="0.0" a="1"/>
        </MATERIAL>

    </MATERIALS>


    <!-- The leaves -->
    <LEAVES>
    </LEAVES>


    <!-- The nodes -->
    <NODES>

        <ROOT id="root"/>

        <!-- the root node-->
        <NODE id="root">
            <MATERIAL id="defaultMaterial"/>
            <TEXTURE id="null"/>

            <DESCENDANTS>
                <NODEREF id="chao"/>
                <NODEREF id="bandeiraCompleta"/>
                <NODEREF id="apollo11"/>
                <NODEREF id="background"/>
                <NODEREF id="background2"/>
                <NODEREF id="background3"/>
